{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "africastalking/africastalking": "^2.3",
        "barryvdh/laravel-cors": "^0.11.4",
        "bref/bref": "^0.5.11",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.4",
        "joselfonseca/lighthouse-graphql-passport-auth": "^2.0",
        "laravel/framework": "^6.0",
        "laravel/passport": "^7.5",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "myclabs/php-enum": "^1.7",
        "nuwave/lighthouse": "^4.3",
        "phpseclib/bcmath_compat": "1.0.4",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "haydenpierce/class-finder": "^0.4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "if [ \"$OAUTH_PUBLIC_KEY\" != \"\" ]; then echo \"$OAUTH_PUBLIC_KEY\" > storage/oauth-public.key; fi",
            "if [ \"$OAUTH_PRIVATE_KEY\" != \"\" ]; then echo \"$OAUTH_PRIVATE_KEY\" > storage/oauth-private.key; fi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
